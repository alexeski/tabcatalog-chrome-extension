{
	"manifest_version": 2,
	"name": "Tableau Metadata API Explorer",
	"version": "0.0.1",
	"permissions": ["<all_urls>"],
	"background": {
		"scripts": ["background.js"]
	},
	"browser_action": {
		"default_popup": "index.html"
	},
	"icons": {
		"16": "images/tableau16.png",
		"48": "images/tableau48.png",
		"128": "images/tableau128.png"
	}
}